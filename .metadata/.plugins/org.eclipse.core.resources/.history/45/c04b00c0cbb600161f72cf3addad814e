package assignment5;

public class Ass5Improved {
	
	public int calculateIndexedFibonacciNumber (int fibIndex) {
		int fibNumberLess1;
		int fibNumberLess2;

		int fibNumber = 1;
		if (fibIndex <= 0) {
			error;
		}
		if (fibIndex <= 2) {
			return fibNumber;
		}
		fibNumberLess2 = 1;
		for (int i = 2; i < fibIndex; i++) {
			fibNumberLess1 = fibNumber;
			fibNumber = fibNumber + fibNumberLess2;
			fibNumberLess2 = fibNumberLess1;
		}
		return fibNumber;
	}
}